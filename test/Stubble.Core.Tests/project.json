{
    "version": "1.0.0-*",
    "buildOptions": {
        "warningsAsErrors": false,
        "allowUnsafe": false,
        "keyFile": "Stubble.Tests.snk",
        "xmlDoc": false,
        "copyToOutput": [
            "../../spec/specs/*.json"
        ]
    },
    "dependencies": {
        "xunit": "2.2.0-beta2-build3300",
        "dotnet-test-xunit": "2.2.0-preview2-build1029",
        "Stubble.Core": { "target": "project" },
        "OpenCover": {
            "version": "4.6.519",
            "type": "build"
        },
        "System.Collections.Specialized": "4.0.1",
        "coveralls.net": {
            "version": "0.7.0",
            "type": "build"
        }
    },
    "testRunner": "xunit",
    "frameworks": {
        "netcoreapp1.0": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.0"
                }
            }
        },
        "net46": {
            "dependencies": {
                "Microsoft.NETCore.Platforms": "1.0.1"
            }
        }
    }
}